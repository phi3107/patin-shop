                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-12  ">
                        <div class="container1">
                            <div class="box">
                                <div class="cover"></div>
                                <div class="shadow"></div>
                                <div class="content">
                                    <% String msg = (String) request.getAttribute("alert");
                                    msg = msg==null?"":msg;%>
                                    <form class="form" action="login" method="post">
                                        <h3 class="logo"><i class="fa-solid fa-key"></i></h3>
                                        <h2>Đăng nhập</h2>
                                        <p style="color: red; font-weight: bold"><%=msg%></p>
                                        <div class="inputBox">
                                            <input name="email" type="text" required >
                                            <i class="fa-solid fa-user"></i>
                                            <span>Email</span>
                                        </div>
                                        <div class="inputBox">
                                            <input name="pass" type="password" required>
                                            <i class="fa-solid fa-lock"></i>
                                            <span>Password</span>
                                        </div>
                                        <div class="links">
                                            <a href="#"><i class="fa-solid fa-question"></i> Quên mật khẩu</a>
                                            <a href="dangky.html"><i class="fa-solid fa-user-plus"></i> Đăng ký</a>
                                        </div>
                                        <div class="inputBox">
                                            <input type="submit" value="Login">
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
