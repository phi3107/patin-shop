      .sticky-mobile-menu {
        display: block;
        position: fixed;
        width: 100%;
        margin: 0;
        z-index: 10000;
        top: 0;
        left: 0;
        margin: 0px !important;
      }

      .sticky-mobile-menu .logo {
        position: absolute;
        width: 60px;
        top: 3px;
        left: 5px;
        z-index: 10;
        background: #343a40;
